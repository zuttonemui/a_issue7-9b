<h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
  <% user.each do |user| %>
    <tr>
      <td class="w-25"><%= image_tag user.get_profile_image(80,80) %></td>
      <td><%= user.name %></td>
      <td><%= render 'relationships/btn', user: user %></td>
      <td><%= link_to "Show", user_path(user) %></td>
    </tr>
  <% end %>
  </tbody>
</table>